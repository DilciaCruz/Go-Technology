	<subDataset name="Concepto a Cotizar" uuid="44713619-552d-4476-9f73-f4b329cdc5cd">
		<queryString language="SQL">
			<![CDATA[SELECT
     proyectos.`codigoProyecto` AS proyectos_codigoProyecto,
     detallecotizaciones.`cantidad` AS detallecotizaciones_cantidad,
     detallecotizaciones.`precio` AS detallecotizaciones_precio,
     detallecotizaciones.`descripcionDetalle` AS detallecotizaciones_descripcionDetalle
FROM
     `proyectos` proyectos,
     `detallecotizaciones` detallecotizaciones]]>
		</queryString>
		<field name="proyectos_codigoProyecto" class="java.lang.Integer"/>
		<field name="detallecotizaciones_descripcionDetalle" class="java.lang.String"/>
		<field name="detallecotizaciones_cantidad" class="java.lang.Integer"/>
		<field name="detallecotizaciones_precio" class="java.lang.Float"/>
	</subDataset>
	<queryString language="SQL">
		<![CDATA[SELECT
     cotizaciones.`codigoCotizacion` AS cotizaciones_codigoCotizacion,
     cotizaciones.`fechaEmisionCotizacion` AS cotizaciones_fechaEmisionCotizacion,
     cotizaciones.`fechaVigencia` AS cotizaciones_fechaVigencia,
     clientes.`identificacionCliente` AS clientes_identificacionCliente,
     clientes.`nombreCliente` AS clientes_nombreCliente,
     clientes.`direccionCliente` AS clientes_direccionCliente,
     proyectos.`codigoProyecto` AS proyectos_codigoProyecto,
     detallecotizaciones.`cantidad` AS detallecotizaciones_cantidad,
     detallecotizaciones.`precio` AS detallecotizaciones_precio,
     detallecotizaciones.`descripcionDetalle` AS detallecotizaciones_descripcionDetalle
FROM
     `clientes` clientes INNER JOIN `cotizaciones` cotizaciones ON clientes.`codigoCliente` = cotizaciones.`codigoCliente`
     INNER JOIN `proyectos` proyectos ON clientes.`codigoCliente` = proyectos.`codigoCliente`
     INNER JOIN `detallecotizaciones` detallecotizaciones ON cotizaciones.`codigoCotizacion` = detallecotizaciones.`codigoCotizacion`]]>
	</queryString>

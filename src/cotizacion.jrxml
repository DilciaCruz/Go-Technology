	<parameter name="codigoCotizacion" class="java.lang.String"/>
	<queryString>
		<![CDATA[select a.codigoCotizacion, a.fechaEmisionCotizacion, a.fechaVigencia, concat(e.descripcionProducto, '', b.descripcionDetalle) as Descripcion, b.cantidad,b.precio, precio*cantidad as SubTotal, round((precio*cantidad)*a.impuesto,2) as Impuesto, (precio*cantidad+ round((precio*cantidad)*a.impuesto,2)) as Total,  Concat(c.nombreCliente, ' ' ,c.apellidoCliente) as NombreCliente, c.identificacionCliente, c.direccionCliente, concat(d.nombreEmpleado, ' ',d.apellidosEmpleado) as NombreEmpleado from cotizaciones as a inner join detallecotizaciones as b on a.codigoCotizacion=b.codigoCotizacion inner join clientes as c on a.codigoCliente=c.codigoCliente inner join empleados as d on a.codigoEmpleado=d.codigoEmpleado inner join productos as e on e.codigoProducto=b.codigoProducto where a.codigoCotizacion =$P{codigoCotizacion};]]>

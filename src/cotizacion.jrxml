		</parameter>
		<queryString>
			<![CDATA[select a.codigoCotizacion, concat(c.descripcionProducto, '', b.descripcionDetalle) as Descripcion, b.precio, b.cantidad from cotizaciones as a inner join detallecotizaciones as b on a.codigoCotizacion=b.codigoCotizacion inner join productos as c on b.codigoProducto=c.codigoProducto where a.codigoCotizacion=$P{codigoCotizacion};]]>
